{
    "version": 2,
    "env":{
        "DATABASE_URL":"file:./dev.db"
    },
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js",
            "methods":[
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
            
        }
    ]
}